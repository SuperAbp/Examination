{
  "name": "angular-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "build": "npm run ng-high-memory build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "prepare": "cd .. && husky install",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint \"src/**/*.less\" --fix"
  },
  "private": true,
  "dependencies": {
    "@abp/ng.core": "~7.4.0",
    "@abp/ng.oauth": "~7.4.0",
    "@angular/animations": "^16.1.7",
    "@angular/common": "^16.1.7",
    "@angular/compiler": "^16.1.7",
    "@angular/core": "^16.1.7",
    "@angular/forms": "^16.1.7",
    "@angular/platform-browser": "^16.1.7",
    "@angular/platform-browser-dynamic": "^16.1.7",
    "@angular/router": "^16.1.7",
    "@delon/abc": "^16.2.0",
    "@delon/acl": "^16.2.0",
    "@delon/auth": "^16.2.0",
    "@delon/cache": "^16.2.0",
    "@delon/chart": "^16.2.0",
    "@delon/form": "^16.2.0",
    "@delon/mock": "^16.2.0",
    "@delon/theme": "^16.2.0",
    "@delon/util": "^16.2.0",
    "ng-alain": "16.2.0",
    "ng-zorro-antd": "^16.1.0",
    "@super-abp/ng.identity": "~0.4.0",
    "@super-abp/ng.menu-management": "~0.4.0",
    "@super-abp/ng.permission-management": "~0.4.0",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@abp/ng.schematics": "~7.4.0",
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular-eslint/builder": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.1.0",
    "@angular-eslint/eslint-plugin-template": "~16.1.0",
    "@angular-eslint/schematics": "~16.1.0",
    "@angular-eslint/template-parser": "~16.1.0",
    "@angular/cli": "~16.1.6",
    "@angular/compiler-cli": "^16.1.7",
    "@angular/language-service": "^16.1.0",
    "@delon/testing": "^16.2.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "~6.1.0",
    "@typescript-eslint/parser": "~6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-deprecation": "~1.4.1",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~46.4.4",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~5.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.0.4",
    "ng-alain": "^16.2.0",
    "ng-alain-plugin-theme": "^15.0.1"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "npm run lint:style"
    ]
  }
}